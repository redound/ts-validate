{
  "version": "1.8.9",
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "noImplicitAny": false,
    "removeComments": true,
    "noLib": false,
    "preserveConstEnums": true,
    "declaration": true,
    "suppressImplicitAnyIndexErrors": true,
    "outDir": "./lib"
  },
  "filesGlob": [
    "./**/*.ts",
    "!./node_modules/**/*.ts"
